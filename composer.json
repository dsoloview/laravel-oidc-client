{
  "name": "dsoloview/laravel-oidc-client",
  "description": "OpenID Connect client for Laravel",
  "keywords": ["OpenID Connect", "oidc", "Laravel"],
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.1",
    "laravel/framework": ">=9",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload" : {
    "psr-4": {
      "Dsoloview\\LaravelOIDC\\": "src/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "Dsoloview\\LaravelOIDC\\Providers\\LaravelProvider"
      ]
    }

  }
}
